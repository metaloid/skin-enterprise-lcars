<?xml version="1.0" encoding="UTF-8"?>
<window id="145">
    <defaultcontrol always="true">11</defaultcontrol>
    <allowoverlay>no</allowoverlay>
    <backgroundcolor>background</backgroundcolor>
    <include>OpenClose</include>
    <controls>
        <control type="group">
            <posx>555</posx>
            <top>0</top>
            <control type="image">
                <top>-81</top>
                <width>810</width>
                <height>440</height>
                <texture border="140" >images/Dialog.png</texture>
            </control>
            <control type="label" id="10" description="Heading">
                <posx>210</posx>
                <top>45</top>
                <width>390</width>
                <align>center</align>
                <font>LCARS20</font>
                <textcolor>White</textcolor>
                <animation effect="fade" time="200">VisibleChange</animation>
            </control>
            <control type="slider" id="11" description="Slider">
                <posx>210</posx>
                <top>120</top>
                <width>390</width>
                <onup>noop</onup>
                <ondown>noop</ondown>
                <onleft>11</onleft>
                <onright>11</onright>
                <texturesliderbar>images/slider_bg.png</texturesliderbar>
                <!--textureslidernib>images/slider_nib.png</textureslidernib-->
                <textureslidernibfocus>images/slider_nib.png</textureslidernibfocus>
            </control>
            <control type="label" id="12" description="Slider value">
                <posx>110</posx>
                <top>150</top>
                <width>600</width>
                <align>center</align>
                <font>LCARS20</font>
                <textcolor>White</textcolor>

                <animation effect="fade" time="200">VisibleChange</animation>
            </control>
        </control>
    </controls>
</window>
