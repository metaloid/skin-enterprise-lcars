<?xml version="1.0" encoding="UTF-8"?>
<window type="dialog" id="134">
    <defaultcontrol always="true">450</defaultcontrol>
    <allowoverlay>no</allowoverlay>
    <onunload>RunScript(script.favourites)</onunload>
    <controls>
        <control type="panel" id="450">
			<include>NextPos</include>
       		<pagecontrol>66</pagecontrol>
        </control>
		<control type="grouplist" id="5000">
			<top>315</top>
            <posx>625</posx>
			<align>left</align>
            <orientation>vertical</orientation>
            <onup>noop</onup>
			<onright>450</onright>
			<ondown>noop</ondown>
			<onleft>close</onleft>
			<itemgap>10</itemgap>
			<control type="button" id="677">
				<onclick>back</onclick>
				<!--onclick>ReloadSkin()</onclick-->
				<width>250</width>
				<label>Close</label>
			</control>
			<control type="spincontrolex" id="66">
				<description>Next page button</description>
				<visible>[Container(450).HasPrevious |  Container(450).HasNext]</visible>
				<width>258</width>
				<subtype>page</subtype>
				<font>-</font>
				<textcolor>-</textcolor>
				<showonepage>true</showonepage>
				<spinwidth>125</spinwidth>
				<spinheight>40</spinheight>
				<texturenofocus></texturenofocus>
				<texturefocus></texturefocus>
				<textureup colordiffuse="HellOrange" >down1.png</textureup>
				<texturedown colordiffuse="HellOrange">up1.png</texturedown>
				<textureupfocus colordiffuse="HellOrange">downp.png</textureupfocus>
				<texturedownfocus colordiffuse="HellOrange">upp.png</texturedownfocus>
			</control>    
		</control>		
    </controls>
</window>
